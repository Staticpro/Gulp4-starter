section.map
  .map__bg
    img(src="static/img/general/map.jpg").map__bg-img
  .map__contacts.contacts
    .contacts__title.subtitle
      | Контакты
    .contacts__skype
      svg.contacts__skype-icon.contact__icon
        use(xlink:href="static/img/svg/symbol/sprite.svg#skype")
      p.contacts__text my skype
    .contacts__mail
      svg.contacts__mail-icon.contact__icon
        use(xlink:href="static/img/svg/symbol/sprite.svg#mail")
      p.contacts__text my mail
    .contacts__tel
      svg.contacts__tel-icon.contact__icon
        use(xlink:href="static/img/svg/symbol/sprite.svg#tel")
      p.contacts__text my tel
    .contacts__adress
      svg.contacts__adress-icon.contact__icon
        use(xlink:href="static/img/svg/symbol/sprite.svg#flag")
      p.contacts__text my adress
  .map__flag
    svg.contacts__flag-icon
      use(xlink:href="static/img/svg/symbol/sprite.svg#flag")


